<?xml version="1.0" encoding="utf-8"?>

<!--For Favorite Selector in Part 2 of project -->
<selector xmlns:android="http://schemas.android.com/apk/res/android"
          android:constantSize="false">

    <!-- Disabled-->
    <item
        android:drawable="@drawable/ic_star_outline_grey_700_48dp"
        android:state_enabled="false"/>

    <!-- Default-->
    <item
        android:drawable="@drawable/ic_star_outline_black_48dp"
        android:state_checked="false"
        android:state_enabled="true"
        />

    <!-- Selected-->
    <item
        android:drawable="@drawable/ic_star_yellow_700_48dp"
        android:state_checked="true"
        android:state_enabled="true"
        />

</selector>